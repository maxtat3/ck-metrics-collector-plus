<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<fx:root type="VBox"
         xmlns="http://javafx.com/javafx"
         xmlns:fx="http://javafx.com/fxml"
         spacing="10">
    <fx:include source="AlertLabel.fxml" styleClass="alertDanger"
                alignment="CENTER"
                text="This will delete *EVERYTHING* unique to this group."/>
    <HBox spacing="10" alignment="CENTER_RIGHT">
        <Button fx:id="btnClose" styleClass="btnLight" text="No, Close" />
        <Button fx:id="btnDelete" styleClass="btnDanger" text="Yes, Delete" />
    </HBox>
</fx:root>
