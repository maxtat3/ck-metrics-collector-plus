<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<fx:root type="VBox"
         xmlns="http://javafx.com/javafx"
         xmlns:fx="http://javafx.com/fxml"
         spacing="10">
    <fx:include source="AlertLabel.fxml" styleClass="alertWarning"
                fx:id="alertError"
                alignment="CENTER"
                text="Warning: This removes selected GroupItems and data." />
    <HBox spacing="10" alignment="CENTER_RIGHT">
        <Button fx:id="btnClose" styleClass="btnLight" text="Close" />
        <Button fx:id="btnSubmit" styleClass="btnWarning" text="Remove Item(s)" />
    </HBox>
</fx:root>
